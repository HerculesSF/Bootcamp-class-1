<script>
    window.theme = window.theme || {};
    window.theme.shopifyDate = {
        day: {{ 'now' | date: '%d' }},
        month: {{ 'now' | date: '%m' }},
        year: {{ 'now' | date: '%Y' }}
    };
</script>

<event-subtitle 
    data-event-name="{{ eventName | handleize }}"
    data-live-label="{{ showLiveLabel }}"
    data-coming-soon-date="{{ showComingSoonDate }}"
    data-intersection-observer="{{ enableIntersectionObserver }}">
</event-subtitle>